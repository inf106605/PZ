<userDefinedSQL>SELECT player.*,&lt;br/&gt;  COUNT(*)         AS games_count,&lt;br/&gt;  COUNT(*)         AS win_count,&lt;br/&gt;  COUNT(*)         AS walkover_count,&lt;br/&gt;  SUM(score.score) AS points_sum,&lt;br/&gt;  AVG(score.score) AS points_avg&lt;br/&gt;FROM player&lt;br/&gt;INNER JOIN score&lt;br/&gt;ON player.id_player = score.id_player&lt;br/&gt;INNER JOIN score win_score&lt;br/&gt;ON player.id_player = win_score.id_player&lt;br/&gt;INNER JOIN score walkover_score&lt;br/&gt;ON player.id_player         = walkover_score.id_player&lt;br/&gt;WHERE win_score.place       = 1&lt;br/&gt;AND walkover_score.walkover = &apos;true&apos;</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;Select player.*, COUNT(*) As games_count, COUNT(*) As win_count, COUNT(*) As walkover_count, Sum(score.score) As points_sum, Avg(score.score) As points_avg From player Inner Join score On player.id_player = score.id_player Inner Join score win_score On player.id_player = win_score.id_player Inner Join score walkover_score On player.id_player = walkover_score.id_player Where win_score.place = 1 And walkover_score.walkover = &apos;true&apos;&lt;/SQL&gt;&lt;SubQueries&gt;&lt;SubQuery SQL=&quot;Select player.*, COUNT(*) As games_count, COUNT(*) As win_count, COUNT(*) As walkover_count, Sum(score.score) As points_sum, Avg(score.score) As points_avg From player Inner Join score On player.id_player = score.id_player Inner Join score win_score On player.id_player = win_score.id_player Inner Join score walkover_score On player.id_player = walkover_score.id_player Where win_score.place = 1 And walkover_score.walkover = &apos;true&apos;&quot;&gt;&lt;UnionSubQuery&gt;&lt;DataSource SQL=&quot;player&quot; X=&quot;50&quot; Y=&quot;20&quot; Height=&quot;155&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score&quot; X=&quot;240&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score win_score&quot; X=&quot;430&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score walkover_score&quot; X=&quot;620&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;/UnionSubQuery&gt;&lt;/SubQuery&gt;&lt;/SubQueries&gt;&lt;/ActiveQueryBuilderLayout&gt;</layoutSQL>
