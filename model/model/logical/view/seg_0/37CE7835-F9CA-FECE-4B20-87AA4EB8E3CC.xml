<userDefinedSQL>SELECT player.*,&lt;br/&gt;  COUNT(*)                      AS walkover_count,&lt;br/&gt;  COUNT(*)                      AS games_count,&lt;br/&gt;  AVG(non_walkover_score.score) AS avg_score&lt;br/&gt;FROM player&lt;br/&gt;INNER JOIN score walkover_score&lt;br/&gt;ON player.id_player = walkover_score.id_player&lt;br/&gt;INNER JOIN score games_score&lt;br/&gt;ON player.id_player = games_score.id_player&lt;br/&gt;INNER JOIN score non_walkover_score&lt;br/&gt;ON player.id_player             = non_walkover_score.id_player&lt;br/&gt;WHERE walkover_score.walkover   = &apos;true&apos;&lt;br/&gt;AND non_walkover_score.walkover = &apos;false&apos;</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;Select player.*, COUNT(*) As walkover_count, COUNT(*) As games_count, Avg(non_walkover_score.score) As avg_score From player Inner Join score walkover_score On player.id_player = walkover_score.id_player Inner Join score games_score On player.id_player = games_score.id_player Inner Join score non_walkover_score On player.id_player = non_walkover_score.id_player Where walkover_score.walkover = &apos;true&apos; And non_walkover_score.walkover = &apos;false&apos;&lt;/SQL&gt;&lt;SubQueries&gt;&lt;SubQuery SQL=&quot;Select player.*, COUNT(*) As walkover_count, COUNT(*) As games_count, Avg(non_walkover_score.score) As avg_score From player Inner Join score walkover_score On player.id_player = walkover_score.id_player Inner Join score games_score On player.id_player = games_score.id_player Inner Join score non_walkover_score On player.id_player = non_walkover_score.id_player Where walkover_score.walkover = &apos;true&apos; And non_walkover_score.walkover = &apos;false&apos;&quot;&gt;&lt;UnionSubQuery&gt;&lt;DataSource SQL=&quot;player&quot; X=&quot;50&quot; Y=&quot;20&quot; Height=&quot;83&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score walkover_score&quot; X=&quot;240&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score games_score&quot; X=&quot;430&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score non_walkover_score&quot; X=&quot;620&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;/UnionSubQuery&gt;&lt;/SubQuery&gt;&lt;/SubQueries&gt;&lt;/ActiveQueryBuilderLayout&gt;</layoutSQL>
