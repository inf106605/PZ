<userDefinedSQL>SELECT game.*,&lt;br/&gt;  COUNT(*)               AS player_count,&lt;br/&gt;  SUM(score.score)       AS score_sum,&lt;br/&gt;  winner_score.id_player AS id_winner&lt;br/&gt;FROM game&lt;br/&gt;INNER JOIN score&lt;br/&gt;ON game.id_game = score.id_game&lt;br/&gt;INNER JOIN score winner_score&lt;br/&gt;ON game.id_game          = winner_score.id_game&lt;br/&gt;WHERE winner_score.place = 1&lt;br/&gt;GROUP BY winner_score.id_player</userDefinedSQL>
<layoutSQL>&lt;?xml version = &apos;1.0&apos; encoding = &apos;UTF-8&apos;?&gt;&lt;br&gt;&lt;ActiveQueryBuilderLayout&gt;&lt;SQL&gt;Select game.*, COUNT(*) As player_count, Sum(score.score) As score_sum, winner_score.id_player As id_winner From game Inner Join score On game.id_game = score.id_game Inner Join score winner_score On game.id_game = winner_score.id_game Where winner_score.place = 1 Group By winner_score.id_player&lt;/SQL&gt;&lt;SubQueries&gt;&lt;SubQuery SQL=&quot;Select game.*, COUNT(*) As player_count, Sum(score.score) As score_sum, winner_score.id_player As id_winner From game Inner Join score On game.id_game = score.id_game Inner Join score winner_score On game.id_game = winner_score.id_game Where winner_score.place = 1 Group By winner_score.id_player&quot;&gt;&lt;UnionSubQuery&gt;&lt;DataSource SQL=&quot;game&quot; X=&quot;50&quot; Y=&quot;20&quot; Height=&quot;101&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score&quot; X=&quot;240&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;DataSource SQL=&quot;score winner_score&quot; X=&quot;430&quot; Y=&quot;20&quot; Height=&quot;137&quot; Width=&quot;140&quot;/&gt;&lt;/UnionSubQuery&gt;&lt;/SubQuery&gt;&lt;/SubQueries&gt;&lt;/ActiveQueryBuilderLayout&gt;</layoutSQL>
